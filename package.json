{
  "name": "mcp-doca-v2",
  "version": "1.0.0",
  "description": "MCP Server para DOCA Agência IA - Agente WhatsApp com WAHA",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.server.json",
    "start": "node dist/index.js",
    "webhook": "node dist/webhook.js",
    "dev": "tsx watch src/index.ts",
    "dev:webhook": "tsx watch src/webhook.ts",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "inspect": "node --inspect dist/index.js"
  },
  "keywords": [
    "mcp",
    "whatsapp",
    "waha",
    "ai",
    "automation",
    "doca"
  ],
  "author": "DOCA Agência IA",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.90.1",
    "dotenv": "^17.2.3",
    "googleapis": "^169.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "luxon": "^3.7.2",
    "pixi.js": "^8.14.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/html2canvas": "^0.5.35",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
